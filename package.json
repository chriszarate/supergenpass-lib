{
  "name": "supergenpass-lib",
  "version": "3.0.0",
  "description": "Generate passwords with SuperGenPass",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    },
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "es2015"
    ]
  },
  "ava": {
    "require": [
      "babel-core/register"
    ]
  },
  "nyc": {
    "instrument": false,
    "require": [
      "babel-core/register"
    ],
    "sourceMap": false
  },
  "scripts": {
    "compile": "babel src --out-dir dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coverage-html": "nyc report --reporter=html",
    "lint": "eslint src tests",
    "prepublish": "npm run compile",
    "test": "BABEL_ENV=test nyc ava tests"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/chriszarate/supergenpass-lib.git"
  },
  "author": "Chris Zarate",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/chriszarate/supergenpass-lib/issues"
  },
  "homepage": "https://github.com/chriszarate/supergenpass-lib",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "coveralls": "^2.11.11",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "nyc": "^7.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "crypto-js": "^3.1.6"
  }
}
